<?xml version="1.0"?>
<data_managers>
    <data_manager tool_file="data_manager/data_manager_groot_database_downloader.xml" id="groot_database_downloader">
        <data_table name="groot_database">
            <output>
                <column name="value" />
                <column name="name" />
                <column name="dbkey" />
                <column name="path" output_ref="out_file" >
                    <move type="directory" relativize_symlinks="True">
                        <target base="${GALAXY_DATA_MANAGER_DATA_PATH}">groot_database/${name}</target>
                    </move>
                    <value_translation>${GALAXY_DATA_MANAGER_DATA_PATH}/groot_database/${name}/${path}</value_translation>
                    <value_translation type="function">abspath</value_translation>
                </column>
            </output>
        </data_table>
    </data_manager>
</data_managers>